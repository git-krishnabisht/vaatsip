{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_API_BASE": "https://vaatsip-web.onrender.com/api",
    "VITE_WS_API_BASE": "wss://vaatsip-web.onrender.com/ws"
  }
}
